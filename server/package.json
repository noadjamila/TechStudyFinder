{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "main": "index.ts",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:unit": "cross-env NODE_ENV=test jest --testMatch='**/*.test.ts' --testPathIgnorePatterns='\\.integration\\.test\\.ts$' --passWithNoTests",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.js --passWithNoTests",
    "start": "cross-env NODE_ENV=development npx ts-node index.ts",
    "dev": "cross-env NODE_ENV=development nodemon index.ts",
    "build": "tsc",
    "serve": "cross-env NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "express-async-errors": "^3.1.1",
    "jest": "^29.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2"
  }
