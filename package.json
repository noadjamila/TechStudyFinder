{
  "name": "tech-study-finder",
  "private": true,
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm start --workspace server\" \"npm start --workspace client\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "build": "npm run build --workspace server && npm run build --workspace client",
    "test": "npm test --workspaces"
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^8.2.0",
    "eslint": "^9.38.0",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "source-map": "^0.7.4",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "server/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "client/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "express-async-error": "^0.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "^5.0.1"
  }
}
