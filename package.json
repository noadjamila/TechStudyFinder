{
  "name": "tech-study-finder",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm start --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "build": "npm run build --workspace=server && npm run build --workspace=client",
    "test": "concurrently \"npm run test:client\" \"npm run test:server\"",
    "test:client": "npm run test --workspace=client",
    "test:server": "npm run test --workspace=server",
    "test:integration": "npm run test:integration --workspace=server",
    "sbom": "npm sbom --sbom-format spdx --sbom-type application > sbom.spdx.json"
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.15",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.3",
    "react-refresh": "^0.18.0",
    "source-map": "^0.7.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "server/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "client/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.7",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  }
}
